    "@types/lodash-es": "^4.17.4",
    "@types/nprogress": "^0.2.0",
    "@types/numeral": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "~5.0.0-alpha.3",
    "@vue/cli-plugin-eslint": "~5.0.0-alpha.3",
